#include<bits/stdc++.h>
#include<list>
#include<iterator>
#include<algorithm>
#include<cmath>
#include<string.h>
#include<vector>
#include<time.h>

#define longpro int64_t ;
#define AMAI ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define ll long long

using namespace std ;

int main()
{
    AMAI ;

    ll T , i,j,  num ;
    for(j = 1 ; ; j++)
    {
        cin >> T ;
        if(T == 0) break ;

        ll sum = 0 ;
        for(i = 0 ; i < T ; i++)
        {
            cin >> num ;
            if( num != 0 )
            {
                sum ++ ;
            }
            else
                sum-- ;

        }
        cout << "Case " << j << ": " ;
        cout << sum << endl;


    }





return 0 ;
}

